ccm.callback["templates.json"]({
  "main": {
    "tag": "div",
    "class": "main-content", 
    "inner": [
      {
        "tag":"div",
        "class":"add_story"
      },
      {
        "tag":"div",
        "class":"sort_story"
      },
      {
        "tag":"div",
        "class":"story_entries"
      }
    ]
  },

  "sort": {
    "tag": "div",
    "class": "sort_div", 
    "inner": [ 
      {
        "tag":"div",
        "class":"sort_navi",
        "inner": [
          {
            "inner": "Nach:"
          },
          {
            "tag": "select",
            "class":"sort_selection selection",
            "inner": [
              {
                "tag":"option",
                "value":"value",
                "inner": "Mehrwert"
              },
              {
                "tag":"option",
                "value":"punish",
                "inner": "Strafe"
              },
              {
                "tag":"option",
                "value":"risk",
                "inner": "Risiko"
              },
              {
                "tag":"option",
                "value":"effort",
                "inner": "Aufwand"
              },
              {
                "tag":"option",
                "value":"prio",
                "inner": "Priorität"
              }
            ]
          },
          {
            "tag": "input",
            "class":"sort_button button",
            "type": "submit",
            "value": "Sortieren"
          }
        ]
      },
      {
        "tag":"div",
        "class":"search_navi",
        "inner": [
          {
            "tag": "select",
            "class":"search_selection selection",
            "inner": [
              {
                "tag":"option",
                "value":"value",
                "inner": "Mehrwert"
              },
              {
                "tag":"option",
                "value":"punish",
                "inner": "Strafe"
              },
              {
                "tag":"option",
                "value":"risk",
                "inner": "Risiko"
              },
              {
                "tag":"option",
                "value":"effort",
                "inner": "Aufwand"
              },
              {
                "tag":"option",
                "value":"prio",
                "inner": "Priorität"
              }
            ]
          },
          {
            "tag":"input",
            "class":"search_input"
          },
          {
            "tag": "input",
            "class":"search_button button",
            "type": "submit",
            "value": "Suchen"
          }
        ]
      }
    ]
  },

  "input": {
    "tag": "div",
    "class": "new_story",
    "inner": {
      "tag": "form",
      "inner": [
        {
          "tag":"div",
          "class":"story_text",
          "inner": [
            {
              "tag":"div",
              "class":"text",
              "inner" : "Userstory:"
            },
            {
              "tag": "textarea",
              "cols":"44",
              "rows":"6",
              "placeholder": "Hier Story eingeben..."
            }
          ]
        },
        {
          "tag":"div",
          "class":"selections",
          "inner": [
            {
              "tag":"div",
              "class":"story_value felder",
              "inner": [
                {
                  "tag":"div",
                  "class":"text",
                  "inner" : "Mehrwert:"
                },
                {
                  "tag": "select",
                  "class":"selection selection_value",
                  "inner": [
                    {
                      "tag":"option",
                      "value":"1",
                      "inner": "1"
                    },
                    {
                      "tag":"option",
                      "value":"2",
                      "inner": "2"
                    },
                    {
                      "tag":"option",
                      "value":"3",
                      "inner": "3"
                    },
                    {
                      "tag":"option",
                      "value":"4",
                      "inner": "4"
                    },
                    {
                      "tag":"option",
                      "value":"5",
                      "inner": "5"
                    }
                  ]
                }
              ]
            },
            {
              "tag":"div",
              "class":"story_punish felder",
              "inner": [
                {
                  "tag":"div",
                  "class":"text",
                  "inner" : "Strafe:"
                },
                {
                  "tag": "select",
                  "class":"selection selection_punish",
                  "inner": [
                    {
                      "tag":"option",
                      "value":"1",
                      "inner": "1"
                    },
                    {
                      "tag":"option",
                      "value":"2",
                      "inner": "2"
                    },
                    {
                      "tag":"option",
                      "value":"3",
                      "inner": "3"
                    },
                    {
                      "tag":"option",
                      "value":"4",
                      "inner": "4"
                    },
                    {
                      "tag":"option",
                      "value":"5",
                      "inner": "5"
                    }
                  ]
                }
              ]
            },
            {
              "tag":"div",
              "class":"story_risk felder",
              "inner": [
                {
                  "tag":"div",
                  "class":"text",
                  "inner" : "Risiko:"
                },
                {
                  "tag": "select",
                  "class":"selection selection_risk",
                  "inner": [
                    {
                      "tag":"option",
                      "value":"1",
                      "inner": "1"
                    },
                    {
                      "tag":"option",
                      "value":"2",
                      "inner": "2"
                    },
                    {
                      "tag":"option",
                      "value":"3",
                      "inner": "3"
                    },
                    {
                      "tag":"option",
                      "value":"4",
                      "inner": "4"
                    },
                    {
                      "tag":"option",
                      "value":"5",
                      "inner": "5"
                    }
                  ]
                }
              ]
            },
            {
              "tag":"div",
              "class":"story_effort felder",
              "inner": [
                {
                  "tag":"div",
                  "class":"text",
                  "inner" : "Aufwand:"
                },
                {
                  "tag": "select",
                  "class":"selection_right selection_effort",
                  "inner": [
                    {
                      "tag":"option",
                      "value":"0",
                      "inner": "0"
                    },
                    {
                      "tag":"option",
                      "value":"1",
                      "inner": "1"
                    },
                    {
                      "tag":"option",
                      "value":"3",
                      "inner": "3"
                    },
                    {
                      "tag":"option",
                      "value":"5",
                      "inner": "5"
                    },
                    {
                      "tag":"option",
                      "value":"8",
                      "inner": "8"
                    },
                    {
                      "tag":"option",
                      "value":"13",
                      "inner": "13"
                    },
                    {
                      "tag":"option",
                      "value":"20",
                      "inner": "20"
                    },
                    {
                      "tag":"option",
                      "value":"40",
                      "inner": "40"
                    },
                    {
                      "tag":"option",
                      "value":"100",
                      "inner": "100"
                    }
                  ]
                }
              ]
            }


          ]
        },
        {
          "tag":"div",
          "class":"input_button_div",
          "inner": {
            "tag": "input",
            "class":"input_button",
            "type": "submit",
            "value": "Eintragen"
          }
        }
      ],
      "onsubmit": "%onsubmit%"
    }
  },

  "stories": {
    "tag": "div",
    "class": "stories",
    "inner": [
      {
        "tag": "div",
        "class": "name",
        "inner": "%name%"
      },
      {
        "tag": "div",
        "class": "text",
        "inner": "%story%"
      },
      {
        "tag": "div",
        "class": "value",
        "inner": "Mehrwert: %value%"
      },
      {
        "tag": "div",
        "class": "punish",
        "inner": "Strafe: %punish%"
      },
      {
        "tag": "div",
        "class": "risk",
        "inner": "Risiko: %risk%"
      },
      {
        "tag": "div",
        "class": "effort",
        "inner": "Aufwand: %effort%"
      },
      {
        "tag": "div",
        "class": "prio",
        "inner": "Priorität: %prio%"
      }
    ]
  }
  
});